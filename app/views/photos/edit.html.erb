<div class="container">
  <% provide(:title, "Editing Photo") %>
  <h1><%= yield(:title) %></h1>

  <%= render 'form', photo: @photo %>

  <p>
    <%= link_to 'Show', @photo, class: 'btn btn-default' %>
    <%= link_to photos_path, class: 'btn btn-default' do %>
      <i class="fa fa-chevron-left"></i> Back
    <% end %>
  </p>
</div>